<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="140dp"
    android:layout_height="150dp"
    tools:context=".homeFragment.StayFragment">

    <LinearLayout
        android:background="@drawable/bank_list"
        android:layout_width="140dp"
        android:layout_height="150dp"
        android:orientation="vertical">
        <ImageView
            android:layout_width="140dp"
            android:layout_height="22dp"
            android:background="@drawable/home_stay"/>
        <TextView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:fontFamily="@font/fontm"
            android:text="13"
            android:textSize="15dp"
            android:layout_gravity="right"
            android:layout_marginRight="5dp"
            android:layout_marginTop="4dp"
            android:textColor="#000000"
            android:id="@+id/result_people2"/>
        <TextView
            android:id="@+id/now_stay2"
            android:layout_width="50dp"
            android:layout_height="90dp"
            android:layout_gravity="center"
            android:textColor="#000000"
            android:textSize="80dp"
            android:text="5"
            android:fontFamily="@font/fontm"/>
        <TextView
            android:layout_width="120dp"
            android:layout_height="10dp"
            android:text="광주광역시 광산구 mm은행"
            android:textSize="9dp"
            android:textColor="#000000"
            android:layout_gravity="center"
            android:fontFamily="@font/fontm"/>
    </LinearLayout>

</FrameLayout>